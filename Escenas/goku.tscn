[sub_resource type="GDScript" id="GDScript_xrcn3"]
script/source = "extends CharacterBody3D

@export var SPEED = 5
@export var JUMP_VELOCITY = 4.5
var dir = Vector2.ZERO
var state_machine
var agachado = false
var gravity = ProjectSettings.get_setting(\"physics/3d/default_gravity\")
var fps

func _ready():
	$AnimationTree.active = true
	
func pared():
	if is_on_wall() && velocity.y < 0:
		gravity = 3
		if Input.is_action_just_pressed(\"ui_accept\"):
			velocity.y = JUMP_VELOCITY
	else:
		gravity = 9.8
	
func _anim():
	state_machine = $AnimationTree.get(\"parameters/playback\")
	
	#Correr
	if velocity.x < 0:
		state_machine.travel(\"Correr\")
		$PJ.flip_h = false
	elif velocity.x > 0:
		state_machine.travel(\"Correr\")
		$PJ.flip_h = true
		
	else:
		state_machine.travel(\"Idle\")
	#Salto
	if not is_on_floor():
		if velocity.y < 0:
			state_machine.travel(\"salto_caer\")
		else:
			state_machine.travel(\"Salto\")
	#Pared
	if is_on_wall() && velocity.y < 0:
		state_machine.travel(\"pared_dezlis\")
		$efectos/AnimationPlayer.play(\"tierrita\")
		$efectos.show()
		if Input.is_action_pressed(\"ui_right\"):
			$PJ.flip_h = true
			$efectos.flip_v= true
			
		if Input.is_action_pressed(\"ui_left\"):
			$PJ.flip_h = false
			$efectos.flip_v= false
	else:
		$efectos.hide()
	if Input.is_action_pressed(\"ui_down\") && velocity.x == 0 && velocity.y == 0:
		state_machine.travel(\"Agacharse_inicio\")
	
func _physics_process(delta):
	pared()
	_anim()
	fps = Engine.get_frames_per_second()
	$Interfaz/CanvasLayer/Fps.text = \"FPS: \"+ str(fps)
	if not is_on_floor():
		velocity.y -= gravity * delta

	if Input.is_action_just_pressed(\"ui_accept\") and is_on_floor():
		velocity.y = JUMP_VELOCITY

	dir.x = Input.get_action_strength(\"ui_left\") - Input.get_action_strength(\"ui_right\")
	velocity.x = dir.x * SPEED
	move_and_slide()
"

